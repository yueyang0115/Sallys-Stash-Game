public class GameTest {
  @Test
  public void test_Game() {
    String filename = "./src/test/resources/input.txt";
    try {
      System.setIn(new FileInputStream(filename));
      Game.main();
    } catch (FileNotFoundException e) {
      System.out.println("File not exist");
    }
  }
}
